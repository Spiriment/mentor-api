{
  "name": "aptfuel-api-service",
  "version": "1.0.0",
  "description": "api service for aptfuel platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc && npx tsc-alias && npm run copy-assets",
    "build:staging": "NODE_ENV=staging npx tsc && npx tsc-alias && npm run copy-assets",
    "build:main": "NODE_ENV=production npx tsc && npx tsc-alias && npm run copy-assets",
    "copy-assets": "cp -r src/mails dist/",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "cross-env NODE_ENV=development npm run typeorm migration:generate",
    "migration:run": "cross-env NODE_ENV=development npm run typeorm migration:run",
    "migration:revert": "cross-env NODE_ENV=development npm run typeorm migration:revert",
    "dev": "npm run start:dev",
    "db-seed": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/database/seeders/seed-runner.ts",
    "db-seed:prod": "NODE_ENV=production node dist/database/seeders/seed-runner.js",
    "db-seed:staging": "NODE_ENV=staging node dist/database/seeders/seed-runner.js",
    "build-and-seed:prod": "npm run build:main && npm run db-seed:prod"
  },
  "dependencies": {
    "@bull-board/api": "^6.10.0",
    "@bull-board/express": "^6.10.0",
    "@bull-board/ui": "^6.10.0",
    "@faker-js/faker": "^9.8.0",
    "@types/bull": "^3.15.9",
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.19",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.6.7",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "bullmq": "^5.53.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "form-data": "^4.0.4",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.15.1",
    "nanoid": "^5.1.5",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.3",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.7.1",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/crypto-js": "^4.2.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/nodemailer": "^6.4.17",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
